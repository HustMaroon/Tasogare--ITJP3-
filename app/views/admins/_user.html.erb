<tr id=<%="user-#{user.id}"%>>
	<td><%= index+1%></td>
	<td><%= image_tag user.avatar unless user.avatar.nil? %></td>
	<td><%= link_to user.name, user unless user.name.nil? %></td>
	<td><%= user.email%></td>
	<td><%= link_to('delete', user, method: 'delete', data: {confirm: 'you sure?'}) if user != current_user%> || <%=link_to "view details",user %></td>
</tr>