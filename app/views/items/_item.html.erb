<tr id=<%="item-#{item.id}"%>>
	<td><%= index+1%></td>
	<td><%= item.name%></td>
	<td><%= link_to item.user.name, item.user%></td>
	<td><%= item.approved? ? 'approved': 'unapproved'%>	</td>
	<td><%= link_to('delete', item, method: 'delete', data: {confirm: 'you sure?'}) if current_user.items.include?(item) %> || <%=link_to "view details", item%></td>
</tr>